--tetris code
function tetris_gravity()
    -- collision
    if( block_collision( m_x, m_y + 0.25, m_rotate )) then
        m_score += 40
        set_block()
        reset()
        check_endgame()
    else
        m_y += 1
    end
end

-- clear row ( move blocks above down one unit )
function clear_row( y )
	for ny = y - 1, 7, -1 do
		for nx = 1, 9 do
			mset( nx, ny + 1, mget( nx, ny ))
		end
	end
end
-- solidify block at resting place
function set_block()
	local x_offset = m_block * 4
	local r = m_rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) mset( flr( m_x ) + ( i % 4 ), flr( m_y ) + flr( i / 4 ), 4 + m_block )
			i += 1
		end
	end
end
-- reset for next block
function reset()
	m_y = 5
	m_x = 2
	m_rotate = 0
	m_block_spd = max( 4, 16 - m_level )
	m_block = m_nxt_block
	m_nxt_block = flr( rnd( 7 ))
end
-- check if endgame conditions
function check_endgame()
	for x = 1, 9 do
		if( fget( mget( x, 7 ), 0 )) m_gameover = true
	end
end

-- whole tetris 'block' collision
function block_collision( ax, ay, rotate )
	local b = false
	local x_offset = m_block * 4
	local r = rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) b = b or collision( ax + ( i % 4 ), ay + flr( i / 4 ) )
			i += 1
		end
	end
	return b
end
-- individual sub-block collision
function collision( x, y )
	return solid( x, y ) or solid( x + 0.9, y ) or solid( x + 0.9, y + 0.9 ) or solid( x, y + 0.9 )
end
-- collision - point
function solid( x, y )
	return fget( mget( flr( x ), flr( y ) ), 0 )
end
-- draw 'tetris' block, one sub-block at a time to allow for rotations
function draw_block( drop )
	local x_offset = m_block * 4
	local r = m_rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) then
				if( drop == nil ) then
					spr( 4 + m_block, m_x * 8 + ( i % 4 ) * 8, ( m_y - 4 ) * 8 + flr( i / 4 ) * 8 )
				else
					spr( 18, m_x * 8 + ( i % 4 ) * 8, ( drop - 4 ) * 8 + flr( i / 4 ) * 8 )
				end
			end
			i += 1
		end
	end
end
-- draw ghost of where 'tetris' block will land, one sub-block at a time to allow for rotations
function draw_ghost()
	for i = m_y, 18, 0.25 do
		-- found drop position
		if( block_collision( m_x, i, m_rotate )) do
			draw_block( i - 0.25 )
			break
		end
	end
end

