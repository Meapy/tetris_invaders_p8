function _init()
	c_r = { 1, 1, 0, -1, 1, 1, -1, -1, 0, 1, -1, 1 } -- x, y, flip - clockwise from nw
	shots_setup()

	make_turret()

	new_game()

end

function new_game()
	-- clear blocks
	for y = 5, 18 do
		for x = 1, 9 do
			mset( x, y, mget( 16, 5 ))
		end
	end
	m_x = 2
	m_y = 5
	m_t = 0
	m_level = 0
	m_gameover = false
	m_pause = false
	m_nxt_block = flr( rnd( 7 ))
	m_block = flr( rnd( 7 ))
	m_block_spd = 16
	m_rotate = 1 -- rotate clockwise 0-3 ( 1 = no rotation )
	m_score = 0
end

function _update()

	--loop through all the shots
	--and pass each shot to the
	--update_shot() function.
	--this will make sure that every
	--shot moves or changes every
	--time the game updates.
 foreach(shots,update_shot)

	--move the turret if needed and
	--fire if the player chooses to
 update_turret()
 m_t += 1
 m_block_spd = max( 4, 16 - m_level )
 -- input
 if( btn( 3,0 )) m_block_spd = 2
 if( btnp( 5,0 ) and not block_collision( m_x, m_y, ( m_rotate + 1 ) % 4 )) m_rotate = ( m_rotate + 1 ) % 4
 if( btnp( 0,0 ) or btnp( 1,0 )) then
	 if( btn( 0,0 ) and not block_collision( m_x - 1, m_y, m_rotate )) m_x -= 1
	 if( btn( 1,0 ) and not block_collision( m_x + 1, m_y, m_rotate )) m_x += 1
 end
 -- move down
 if( m_t % m_block_spd == 0 ) tetris_gravity()
 -- check row remove
 local rows = 1
 for y = 7, 18 do
	 for x = 1, 9 do
		 if( not fget( mget( x, y ), 0 )) break
		 if( x == 9 ) then
			 clear_row( y )
			 rows += 1
		 end
	 end
 end
end

function _draw()

 cls()
 map( 0, 4, 0, 0, 128, 128 )
 --loop through all the shots
 --and pass each shot to the
 --draw_shot() function
 foreach(shots,draw_shot)

 --draw the turret and all the
 --enemies that are left
 draw_turret()
 draw_block()
 draw_ghost()
 foreach(enemies,draw_enemy)

 -- next block
 map( m_nxt_block * 4, 0, 88, 8, 4, 4 )
 -- score
 color( 12 )
 cursor( 102, 86 )
 print( m_level )
 cursor( 104 - #sub( m_score, 0 ) * 2, 110 ) -- idk how to convert to string explicitly
 print( m_score )


end
