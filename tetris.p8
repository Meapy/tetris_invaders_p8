pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
-- pico initialization
function _init()
	c_r = { 1, 1, 0, -1, 1, 1, -1, -1, 0, 1, -1, 1 } -- x, y, flip - clockwise from nw
	c_score = { 0, 40, 100, 300, 1200 }
	m_message = 0
	c_message = { "welcome to tetris", "use arrows to move", "press x to rotate", "press z to pause", "press z to start a new game" }
	m_debug = {}
	music( 0 )
	new_game()
end
-- new game
function new_game()
	-- clear blocks
	for y = 5, 18 do
		for x = 1, 9 do
			mset( x, y, mget( 16, 5 ))
		end
	end
	m_x = 2
	m_y = 5
	m_t = 0
	m_level = 0
	m_gameover = false
	m_pause = false
	m_nxt_block = flr( rnd( 7 ))
	m_block = flr( rnd( 7 ))
	m_block_spd = 16
	m_rotate = 0 -- rotate clockwise 0-3 ( 1 = no rotation )
	m_score = 0
end
-- pico update
function _update()
	if( m_gameover ) then
		if( btnp( 4 )) new_game()
	elseif( btnp( 4 )) then
		m_pause = not m_pause
		if( not block_collision( m_x, m_y + 1, m_rotate )) m_y += 1
		m_score -= 20
		m_score = max( 0, m_score )
	elseif( not m_pause ) then
		m_t += 1
		m_block_spd = max( 4, 16 - m_level )
		-- input
		if( btn( 3 )) m_block_spd = 2
		if( btnp( 5 ) and not block_collision( m_x, m_y, ( m_rotate + 1 ) % 4 )) m_rotate = ( m_rotate + 1 ) % 4
		if( btnp( 0 ) or btnp( 1 )) then
			if( btn( 0 ) and not block_collision( m_x - 1, m_y, m_rotate )) m_x -= 1
			if( btn( 1 ) and not block_collision( m_x + 1, m_y, m_rotate )) m_x += 1
		end
		-- move down
		if( m_t % m_block_spd == 0 ) gravity()
		-- check row remove
		local rows = 1
		for y = 7, 18 do
			for x = 1, 9 do
				if( not fget( mget( x, y ), 0 )) break
				if( x == 9 ) then
					clear_row( y )
					rows += 1
				end
			end
		end
		m_score += c_score[ rows ]
	end
	-- level
	if( m_t % 1000 == 0 ) m_level += 1
end
-- pico draw
function _draw()
	cls()
	-- layout
	map( 0, 4, 0, 0, 128, 128 )
	-- next block
	map( m_nxt_block * 4, 0, 88, 8, 4, 4 )
	-- score
	color( 12 )
	cursor( 102, 86 )
	print( m_level )
	cursor( 104 - #sub( m_score, 0 ) * 2, 110 ) -- idk how to convert to string explicitly
	print( m_score )
	-- draw current block
	if( not m_gameover ) draw_ghost()
	if( not m_gameover ) draw_block()
	-- pause
	if( m_pause ) then
		rectfill( 8, 8, 80, 120, 7 )
		color( 12 )
		cursor( 26, 60 )
		print( "- pause -" )
	end
	-- messages
	color( 12 )
	if( m_t % 100 == 0 ) m_message = ( m_message + 1 ) % 4
	if( m_gameover ) m_message = 4
	local offset = #c_message[ m_message + 1 ] / 2
	cursor( 64 - #c_message[ m_message + 1 ] * 2 , 1 )
	print( c_message[ m_message + 1 ] )
	-- debug
	for d in all( m_debug ) do
		rectfill( 0, 120, 127, 127, 8 )
		color( 7 )
		cursor( 0, 122 )
		print( d )
	end
end
-- apply 'gravity' to active player block
function gravity()
	-- collision
	if( block_collision( m_x, m_y + 0.25, m_rotate )) then
		m_score += 40
		set_block()
		reset()
		check_endgame()
	else
		m_y += 1
	end
end
-- clear row ( move blocks above down one unit )
function clear_row( y )
	for ny = y - 1, 7, -1 do
		for nx = 1, 9 do
			mset( nx, ny + 1, mget( nx, ny ))
		end
	end
end
-- solidify block at resting place
function set_block()
	local x_offset = m_block * 4
	local r = m_rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) mset( flr( m_x ) + ( i % 4 ), flr( m_y ) + flr( i / 4 ), 4 + m_block )
			i += 1
		end
	end
end
-- reset for next block
function reset()
	m_y = 5
	m_x = 2
	m_rotate = 0
	m_block_spd = max( 4, 16 - m_level )
	m_block = m_nxt_block
	m_nxt_block = flr( rnd( 7 ))
end
-- check if endgame conditions
function check_endgame()
	for x = 1, 9 do
		if( fget( mget( x, 7 ), 0 )) m_gameover = true
	end
end
-- whole tetris 'block' collision
function block_collision( ax, ay, rotate )
	local b = false
	local x_offset = m_block * 4
	local r = rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) b = b or collision( ax + ( i % 4 ), ay + flr( i / 4 ) )
			i += 1
		end
	end
	return b
end
-- individual sub-block collision
function collision( x, y )
	return solid( x, y ) or solid( x + 0.9, y ) or solid( x + 0.9, y + 0.9 ) or solid( x, y + 0.9 )
end
-- collision - point
function solid( x, y )
	return fget( mget( flr( x ), flr( y ) ), 0 )
end
-- draw 'tetris' block, one sub-block at a time to allow for rotations
function draw_block( drop )
	local x_offset = m_block * 4
	local r = m_rotate * 3 + 1
	-- x, y read direction
	local vx = c_r[ r ]
	local vy = c_r[ r + 1 ]
	local i = 0
	-- read block by block
	for y = abs( min( 0, 3 * vy )), max( 3 * vy, 0 ), vy do
		for x = abs( min( 0, 3 * vx )), max( 3 * vx, 0 ), vx do
			local nx = x
			local ny = y
			-- flip
			if( c_r[ r + 2 ] == 1 ) then
				local temp = nx
				nx = ny
				ny = temp
			end
			if( fget( mget( x_offset + nx, ny ), 0 )) then
				if( drop == nil ) then
					spr( 4 + m_block, m_x * 8 + ( i % 4 ) * 8, ( m_y - 4 ) * 8 + flr( i / 4 ) * 8 )
				else
					spr( 18, m_x * 8 + ( i % 4 ) * 8, ( drop - 4 ) * 8 + flr( i / 4 ) * 8 )
				end
			end
			i += 1
		end
	end
end
-- draw ghost of where 'tetris' block will land, one sub-block at a time to allow for rotations
function draw_ghost()
	for i = m_y, 18, 0.25 do
		-- found drop position
		if( block_collision( m_x, i, m_rotate )) do
			draw_block( i - 0.25 )
			break
		end
	end
end






































